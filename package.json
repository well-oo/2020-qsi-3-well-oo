{
  "name": "hubme",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world",
    "dev": "concurrently -c magenta,cyan \"npm:watch\" \"npm:server\"",
    "watch": "bsb -make-world -w -ws _ ",
    "sync-schema": "graphql get-schema && node ./generateFragmentTypes.js",
    "generate-ppx-schema": "node ./node_modules/gql-tools/cli/gqlschema.js -o graphql_schema.json schema.graphql",
    "clean": "bsb -clean-world",
    "bundle": "webpack",
    "server": "moduleserve ./ --port 8000 --spa",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@baransu/graphql_ppx_re": "^0.6.1",
    "bs-fetch": "^0.5.1",
    "dotenv-webpack": "^1.7.0",
    "gql-tools": "0.0.15",
    "graphql": "^14.6.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": ">=0.7.0",
    "reason-urql": "^1.5.0",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "bs-platform": "^6.2.1",
    "concurrently": "^5.1.0",
    "moduleserve": "^0.9.0",
    "webpack-cli": "^3.3.10"
  }
}
